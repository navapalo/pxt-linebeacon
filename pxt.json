    "license": "MIT",
    "dependencies": {
        "core": "*",
        "bluetooth": "*"
    },
    "files": [
        "enums.d.ts",
        "shims.d.ts",
        "linebeacon.ts",
        "linebeacon.cpp",
        "LineBeaconService.h",
